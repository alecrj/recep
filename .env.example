# Database
DATABASE_URL="postgresql://user:password@localhost:5432/ai_receptionist?schema=public"

# Twilio (Phone & SMS)
TWILIO_ACCOUNT_SID=your_twilio_account_sid
TWILIO_AUTH_TOKEN=your_twilio_auth_token
TWILIO_API_KEY=your_twilio_api_key
TWILIO_API_SECRET=your_twilio_api_secret

# AI Services
DEEPGRAM_API_KEY=your_deepgram_api_key
OPENAI_API_KEY=your_openai_api_key
ELEVENLABS_API_KEY=your_elevenlabs_api_key

# Google Calendar
GOOGLE_CLIENT_ID=your_google_client_id
GOOGLE_CLIENT_SECRET=your_google_client_secret
GOOGLE_REDIRECT_URI=http://localhost:3000/api/auth/google/callback

# Stripe Payment Processing
STRIPE_SECRET_KEY=your_stripe_secret_key
STRIPE_PUBLISHABLE_KEY=your_stripe_publishable_key
STRIPE_WEBHOOK_SECRET=your_stripe_webhook_secret

# Authentication
JWT_SECRET=your_super_secret_jwt_key_change_this_in_production
JWT_EXPIRES_IN=7d

# Application
NODE_ENV=development
PORT=3000
BACKEND_URL=http://localhost:3000
ADMIN_DASHBOARD_URL=http://localhost:5173
BUSINESS_DASHBOARD_URL=http://localhost:5174

# Logging
LOG_LEVEL=debug

# System Admin (Default admin credentials)
ADMIN_EMAIL=admin@yourcompany.com
ADMIN_PASSWORD=change_this_immediately
